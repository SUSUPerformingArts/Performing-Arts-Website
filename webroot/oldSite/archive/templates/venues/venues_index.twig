{# Call the header function to create the standard PA header #}
{{ header("PA Archive :: Shows") }}


    <div class="well well-pa">
        <h1>Performing Arts Venues</h1>
    </div>

    <div class="well well-pa">
    	{% for venue in venues %}
    		<div>
    			<h2><a href="{{ urlFor("venue", { "venue" : venue.id }) }}">{{ venue.name }}</a></h2>
    		</div>
    	{% endfor %}
    </div>

{# Standard PA footer #}
{{ footer() }}
